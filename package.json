{
    "name": "russia-win-win",
    "version": "1.0.0",
    "scripts": {
        "dev": "wireit",
        "build": "wireit",
        "start": "wireit",
        "lint": "wireit"
    },
    "wireit": {
        "dev": {
            "command": "next dev",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "build": {
            "command": "next build",
            "env": {
                "NODE_ENV": "production"
            },
            "cache": {
                "folder": ".next/cache"
            }
        },
        "start": {
            "command": "next start",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "lint": {
            "command": "next lint",
            "env": {
                "NODE_ENV": "development"
            }
        }
    },
    "dependencies": {
        "@types/node": "22.15.30",
        "@types/react": "19.1.8",
        "@types/react-dom": "19.1.6",
        "eslint": "9.37.0",
        "eslint-config-next": "15.5.5",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "million": "^2.6.4",
        "next": "^15.5.5",
        "normalize.css": "^8.0.1",
        "prettier": "^3.6.2",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "styled-components": "^6.1.19",
        "stylis": "^4.3.6",
        "typescript": "5.9.3"
    },
    "packageManager": "yarn@4.9.1",
    "devDependencies": {
        "wireit": "^0.14.12"
    }
}
